<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 300 300" width="300" height="300">
  <!-- Fondo -->
  <rect width="300" height="300" fill="#f5f5f5"/>

  <!-- Mouse -->
  <g transform="translate(75, 70)">
    <!-- Cuerpo del mouse -->
    <path d="M30,50 C30,20 120,20 120,50 L120,120 C120,150 30,150 30,120 Z" fill="#333333" stroke="#000" stroke-width="2"/>

    <!-- Botones -->
    <path d="M30,50 C30,20 75,20 75,50 L75,80 C75,90 30,90 30,80 Z" fill="#444444" stroke="#000" stroke-width="1"/>
    <path d="M75,50 C75,20 120,20 120,50 L120,80 C120,90 75,90 75,80 Z" fill="#444444" stroke="#000" stroke-width="1"/>

    <!-- Rueda de desplazamiento -->
    <rect x="70" y="40" width="10" height="30" rx="5" ry="5" fill="#666666"/>

    <!-- Logo -->
    <text x="75" y="120" font-family="Arial" font-size="12" font-weight="bold" fill="#cccccc" text-anchor="middle">MX</text>
  </g>

  <!-- Texto -->
  <text x="150" y="220" font-family="Arial" font-size="16" font-weight="bold" fill="#000" text-anchor="middle">Mouse Logitech</text>
  <text x="150" y="240" font-family="Arial" font-size="12" fill="#666" text-anchor="middle">MX Master 3</text>
</svg>
