<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 300 300" width="300" height="300">
  <!-- Fondo -->
  <rect width="300" height="300" fill="#f5f5f5"/>

  <!-- Auriculares -->
  <g transform="translate(60, 50)">
    <!-- Diadema -->
    <path d="M30,80 C30,40 150,40 150,80" stroke="#000" stroke-width="8" fill="none"/>

    <!-- Auricular izquierdo -->
    <g transform="translate(20, 80)">
      <ellipse cx="10" cy="30" rx="30" ry="40" fill="#0066cc"/>
      <ellipse cx="10" cy="30" rx="20" ry="30" fill="#003366"/>
      <text x="10" y="30" font-family="Arial" font-size="10" fill="white" text-anchor="middle">JBL</text>
    </g>

    <!-- Auricular derecho -->
    <g transform="translate(150, 80)">
      <ellipse cx="0" cy="30" rx="30" ry="40" fill="#0066cc"/>
      <ellipse cx="0" cy="30" rx="20" ry="30" fill="#003366"/>
      <text x="0" y="30" font-family="Arial" font-size="10" fill="white" text-anchor="middle">JBL</text>
    </g>
  </g>

  <!-- Texto -->
  <text x="150" y="220" font-family="Arial" font-size="16" font-weight="bold" fill="#000" text-anchor="middle">Auriculares JBL</text>
  <text x="150" y="240" font-family="Arial" font-size="12" fill="#666" text-anchor="middle">JBL Tune 500BT</text>
</svg>
